{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/tsdoc/v0/tsdoc.schema.json",
  "extends": ["typedoc/tsdoc.json"],
  "noStandardTags": false,
  "tagDefinitions": [
    {
      "tagName": "@abstract",
      "syntaxKind": "modifier"
    },
    {
      "tagName": "@memberof",
      "syntaxKind": "block"
    },
    {
      "tagName": "@Allow",
      "syntaxKind": "block"
    },
    {
      "tagName": "@typedef",
      "syntaxKind": "block"
    },
    {
      "tagName": "@property",
      "syntaxKind": "block"
    }
  ],
  "supportForTags": {
    "@abstract": true,
    "@memberof": true,
    "@Allow": true,
    "@typedef": true,
    "@property": true
  }
}
